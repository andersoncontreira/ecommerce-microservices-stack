{
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "artillery": "^2.0.23",
    "dotenv": "^17.2.1"
  },
  "scripts": {
    "artillery": "artillery",
    "test:ridely-service:root": "artillery run ./artillery/ridely-service/root.yaml",
    "test:ridely-service:status": "artillery run ./artillery/ridely-service/status.yaml",
    "test:ridely-service:list-drivers": "artillery run ./artillery/ridely-service/list-drivers.yaml",
    "test:ridely-service:list-drivers-no-auth": "artillery run ./artillery/ridely-service/list-drivers-no-auth.yaml",
    "test:ridely-service:list-drivers-60-10": "artillery run ./artillery/ridely-service/list-drivers-60-10.yaml",
    "test:ridely-service:list-drivers-60-10-no-auth": "artillery run ./artillery/ridely-service/list-drivers-60-10-no-auth.yaml",
    "test:ridely-service:list-drivers-60-50": "artillery run ./artillery/ridely-service/list-drivers-60-50.yaml",
    "test:ridely-service:list-drivers-60-50-no-auth": "artillery run ./artillery/ridely-service/list-drivers-60-50-no-auth.yaml",
    "test:ridely-service:estimate-ride": "artillery run ./artillery/ridely-service/estimate-ride.yaml",
    "test:ridely-service:estimate-ride-no-auth": "artillery run ./artillery/ridely-service/estimate-ride-no-auth.yaml",
    "test:ridely-service:request-driver": "artillery run  --output ./artillery/ridely-service/reports/request-driver.json ./artillery/ridely-service/request-driver.yaml",
    "test:ridely-service:request-driver-no-auth": "artillery run --output ./artillery/ridely-service/reports/request-driver-no-auth.json ./artillery/ridely-service/request-driver-no-auth.yaml",
    "test:ridely-service:request-driver-60-10": "artillery run  --output ./artillery/ridely-service/reports/request-driver-60-10.json ./artillery/ridely-service/request-driver-60-10.yaml",
    "test:ridely-service:request-driver-60-10-no-auth": "artillery run  --output ./artillery/ridely-service/reports/request-driver-60-10.json ./artillery/ridely-service/request-driver-60-10-no-auth.yaml",
    "test:ridely-service:request-driver-60-50": "artillery run  --output ./artillery/ridely-service/reports/request-driver-60-50.json ./artillery/ridely-service/request-driver-60-50.yaml",
    "test:ridely-service:request-driver-60-50-no-auth": "artillery run  --output ./artillery/ridely-service/reports/request-driver-60-50.json ./artillery/ridely-service/request-driver-60-50-no-auth.yaml"
  }
}
